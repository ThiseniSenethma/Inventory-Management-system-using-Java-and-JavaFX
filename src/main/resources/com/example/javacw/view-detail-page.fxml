<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="911.0" prefWidth="711.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javacw.NewController">
   <children>
      <TableView fx:id="itemsTable" layoutX="26.0" layoutY="73.0" prefHeight="567.0" prefWidth="898.0">
          <columns>
      
                    <!-- itemCodeColumn -->
              <TableColumn fx:id="itemCodeColumn" prefWidth="56.800015449523926" text="Code">
                  <cellValueFactory>
                      <PropertyValueFactory property="itemCode" />
                  </cellValueFactory>
              </TableColumn>
      
              <!-- itemNameColumn -->
              <TableColumn fx:id="itemNameColumn" prefWidth="116.0" text="Name">
                  <cellValueFactory>
                      <PropertyValueFactory property="itemName" />
                  </cellValueFactory>
              </TableColumn>
      
              <!-- itemBrandColumn -->
              <TableColumn fx:id="itemBrandColumn" prefWidth="80.79995727539062" text="Brand">
                  <cellValueFactory>
                      <PropertyValueFactory property="itemBrand" />
                  </cellValueFactory>
              </TableColumn>
      
              <!-- itemPriceColumn -->
              <TableColumn fx:id="itemPriceColumn" prefWidth="88.80000305175781" text="Price">
                  <cellValueFactory>
                      <PropertyValueFactory property="itemPrice" />
                  </cellValueFactory>
              </TableColumn>
      
              <!-- itemQuantityColumn -->
              <TableColumn fx:id="itemQuantityColumn" text="Quantity">
                  <cellValueFactory>
                      <PropertyValueFactory property="itemQuantity" />
                  </cellValueFactory>
              </TableColumn>
      
                    <!-- itemCategoryColumn -->
              <TableColumn fx:id="itemCategoryColumn" prefWidth="127.20001220703125" text="Category">
                  <cellValueFactory>
                      <PropertyValueFactory property="itemCategory" />
                  </cellValueFactory>
              </TableColumn>
      
              <!-- purchasedDateColumn -->
              <TableColumn fx:id="purchasedDateColumn" prefWidth="120.79998779296875" text="Purchased Date">
                  <cellValueFactory>
                      <PropertyValueFactory property="purchasedDate" />
                  </cellValueFactory>
              </TableColumn>
              <!-- imageColumn -->
              <TableColumn fx:id="imageColumn" prefWidth="227.199951171875" text="Image">
                  <cellValueFactory>
                      <PropertyValueFactory property="image" />
                  </cellValueFactory>
              </TableColumn>
          </columns>
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </TableView>
      <Button fx:id="continuebutton5" layoutX="652.0" layoutY="693.0" mnemonicParsing="false" onAction="#onContinueButton2Click" text="Yes">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="exitbutton6" layoutX="789.0" layoutY="692.0" mnemonicParsing="false" onAction="#onExitButton3Click" prefHeight="30.0" prefWidth="40.0" text="No">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="606.0" layoutY="663.0" prefHeight="18.0" prefWidth="294.0" text="Do you want to continue ?">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Label>
      <Label layoutX="364.0" layoutY="27.0" prefHeight="27.0" prefWidth="222.0" text="Viewing Item Details " textFill="#00b2ca">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Label fx:id="itemTotal" layoutX="43.0" layoutY="655.0" prefHeight="62.0" prefWidth="346.0" text="The Current Total : ">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Button layoutX="416.0" layoutY="673.0" mnemonicParsing="false" onAction="#onCurrentTotalButtonClick" style="-fx-background-color: #00b2ca;" text="Calculate">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
